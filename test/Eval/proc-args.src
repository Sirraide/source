// R %srcc --eval %s
program test;

import <cstdio> as *;

proc a () { printf("a: \n"); }
proc b (int a) { printf("b: %zi\n", a); }
proc c (int a, int b, int c) {
    printf("c: %zi %zi %zi\n", a, b, c);
}
proc d (int x1, int x2, int x3) {
    c(x1, x2, x3);
}
proc e (
    int a, int b, int c, int d, int e,
    int f, int g, int h, int i, int j
) {
    printf(
        "e: %zi %zi %zi %zi %zi %zi %zi %zi %zi %zi\n",
        a, b, c, d, e, f, g, h, i, j
    );
}

// * a:
// + b: 1
// + c: 1 2 3
// + c: 1 2 3
// + e: 1 2 3 4 5 6 7 8 9 0
a();
b(1);
c(1, 2, 3);
d(1, 2, 3);
e(1, 2, 3, 4, 5, 6, 7, 8, 9, 0);