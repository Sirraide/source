// V %srcc --sema --verify %s
program test;

eval {
    int a;
    int b;
    assert &a == &a;
    assert &b == &b;
    assert &a != &b;
    assert &b != &a;
}

eval {
    int a;
    int b;
    assert &a == &b; // expected-error: Assertion failed: '&a == &b'
}

eval {
    int a;
    int b;
    assert &a != &a; // expected-error: Assertion failed: '&a != &a'
}
