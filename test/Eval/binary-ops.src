// R %srcc --eval %s
program test;

proc arith(int a, int b) {
    __srcc_print(a, ", ", b, "\n");
    __srcc_print(a + b, "\n");
    __srcc_print(a +~ b, "\n");
    __srcc_print(a +| b, "\n");

    __srcc_print(a - b, "\n");
    __srcc_print(a -~ b, "\n");
    __srcc_print(a -| b, "\n");

    __srcc_print(a * b, "\n");
    __srcc_print(a *~ b, "\n");
    __srcc_print(a *| b, "\n");

    __srcc_print(a / b, "\n");
    __srcc_print(a % b, "\n");
    __srcc_print(a :/ b, "\n");
    __srcc_print(a :% b, "\n");

    __srcc_print(a ** b, "\n");
}

