proc f1 -> int {
    int a = 20;
    int b = 2;
    proc g = a;
    proc h = 20;
    return g() + h() + 2;
}

proc f2 -> int {
    int a = 20;
    proc g -> int {
        int b = 30;
        proc h = a + b;
        return h();
    }
    return g();
}

proc f3 -> int {
    int a = 22;
    int b = 20;

    proc h(bool recurse) -> int {
        return if recurse then g(false) else a;
    }

    proc g(bool recurse) -> int {
        return if recurse then h(false) else b;
    }

    return h(true) + g(true);
}

proc f4 -> int {
    int a = 1;
    proc g1 = a;
    proc g2 -> int {
        int b = 2;
        proc g3 = a + b;
        proc g4 = g1() + g3() + a + b;
        return g1() + g3() + g4() + a;
    }
    return g1() + g2();
}

