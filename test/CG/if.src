program test;

import <cstdio> as *;

x(1, 2, true);
x(2, 1, false);

proc x(int a, int b, bool d) {
    if d then printf("d = true\n");
    else printf("d = false\n");
    printf("%s", (if d "d = true\n" else "d = false\n").data);

    (if d then d else d) = (if a < b then false else true);

    if d then printf("d = true\n");
    else printf("d = false\n");
    printf("%s", (if d "d = true\n" else "d = false\n").data);

    printf("\n");
}

