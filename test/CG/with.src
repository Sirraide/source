// R %srcc --ir --target x86_64-unknown-linux %s
program test;

struct s {
    int x;
}

proc q() = s(42);
proc sink(int) extern;

s a = (1);
with a { x = 4; }
sink(a.x);

with s(2) {
    sink(x);
    x = 3;
    sink(x);
}

with q() {
    sink(x);
}

