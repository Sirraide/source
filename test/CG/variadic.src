// R %srcc --ir --target x86_64-unknown-linux %s
program test;

proc f1 (int... x) {}
proc q1 {
    f1();
    f1(1);
    f1(1, 2);
    f1(1, 2, 3);
}

proc f2 (i32 y, i32... x) {}
proc q2 {
    f2(42);
    f2(42, 1);
    f2(42, 1, 2);
    f2(42, 1, 2, 3);
}

proc f3 ($T... x) {}
proc q3 {
    f3();
    f3(1);
    f3(1, 2);
    f3(1, 2, 3);
}

proc f4 ($T, $T... x) {}
proc q4 {
    f4(42);
    f4(42, 1);
    f4(42, 1, 2);
    f4(42, 1, 2, 3);
}

proc f5 (var ...x) {}
proc q5 {
    f5();
    f5(1);
    f5(1, 2);
    f5(1, 2);
    f5(1, "");
    f5(1, "");
}

proc f6 (var y, var ...x) {}
proc q6 {
    f6(1);
    f6("", 1);
    f6(4 as i32, 1, 2);
    f6(7 as i47, 1, 2);
    f6(3 as i16, 1, "");
    f6(2 as i128, 1, "");
}

