program test;

struct s { int x; int y; }

proc p1 {
    s? x;

    if x != nil {
        x.x = 1;
        x.y = 2;
    } else {}

    if x == nil {} else {
        x.x = 1;
        x.y = 2;
    }

    assert x != nil;
    x.x = 1;
    x.y = 2;
}

proc p2(in int^? a, in int^? b) {
    if a != nil and b != nil {
        ^a = 3;
        ^b = 3;
    } else {}
}

proc p3(in int^? a, in int^? b) {
    if a == nil or b == nil {} else {
        ^a = 3;
        ^b = 3;
    }
}

