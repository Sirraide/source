// R[//L] %srcc --llvm --target x86_64-unknown-linux %s
program test;

proc while_simple {
    int i;
    while i < 10 do i = i +~ 1;
}

proc while_nested {
    int i;
    while i < 10 do {
        int j;
        while j < 10 do j = j +~ 1;
        i = i +~ 1;
    }
}

proc while_infinite {
    int i;
    while true {
        i = i +~ 1;
    }
}

proc while_infinite_empty {
    while true {}
}

