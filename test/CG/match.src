program test;

proc f1 {
    var x = match true {
        true: 1;
        false: 2;
    };
}

proc f2 {
    var x = match false {
        true: 3;
        false: 4;
    };
}

proc f3 {
    var z = match true {
        true: 1;
        true: 2;
        false: 3;
    };
}

proc f4(bool a, bool b) -> int {
    return match a {
        b: 0;
        true: 1;
        false: 2;
    };
}

proc f5() {
    int x;
    int y;
    int^ z = &match true {
        true: x;
        false: y;
    };
    ^z = 42;
}

proc f6() {
    int x = match true {
        _: 5;
    };
}

proc f7() {
    int x = match true {
        _: 5;
        true: "unreachable";
    };
}

proc f8(bool b) {
    int x = match b {
        true: 7;
        _: 5;
    };
}

proc f9(int i) -> int {
    return match i {
        1..=10: 1;
        _: 2;
    };
}

proc f10(int i) -> int {
    return match i {
        1..=10: 1;
        11..=20: 2;
        21..=30: 3;
        _: 4;
    };
}

proc f11(int i) -> int {
    return match i {
        int.min..<int.max: 1;
        _: 2;
    };
}

proc f12(int i) -> int {
    return match i {
        int.min..<int.max: 1;
        _: 1;
    };
}

