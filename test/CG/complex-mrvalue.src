struct s { i64 x; }

proc a -> s {
    var x = {{{ s(1) }}};
    return {{{ s(1); }}};
}

proc b (in bool x) -> s {
    var y = if x then s(1) else s(2);
    return if x then s(1) else s(2);
}

proc c (in bool x) -> s {
    var y = if x then s(1) else a();
    return if x then s(2) else a();
}

proc d (in bool x) {
    s a = s(1);
    s b = a;
    s c = if x then a else b;
    s d = if x then s(1) else b;
    s e = if x then a else s(1);
}

