// R[//L] %srcc --llvm --target x86_64-unknown-linux -fgc-procs %s
program test;

// Test that we elminate procedures that are never referenced (or only
// referenced at compile-time). '-fgc-procs' is disabled by default if
// '--ir' or '--llvm' is passed, so we need to enable it explicitly.
proc never_called = 42;
proc called_at_compile_time = 42;
int x = eval called_at_compile_time();

