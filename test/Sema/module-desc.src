// R %srcc --exports %s --target x86_64-unknown-linux
// V mkdir %t
// V %srcc %s --mo %t --target x86_64-unknown-linux
// R %srcc --dump-module test -M %t --target x86_64-unknown-linux
module test;

// * f1 proc (i64) -> i32
export proc f1 (i64) -> i32 = 42;

// * f2 proc (i64) -> i32
export proc f2 (i64) nomangle -> i32 = 42;

// * f3 proc (i64) -> i32
export proc f3 (i64) native -> i32 = 42;

// * f5 proc (i64) varargs -> i32
// * f6 proc (i64) varargs -> i32
export proc f5 (i64) native varargs -> i32 = 42;
export proc f6 (i64) varargs native -> i32 = 42;

// * struct s1 size 24/24 align 8
// + i32 a offs 0
// + i53 b offs 8
// + i64 c offs 16
export struct s1 {
    i32 a;
    i53 b;
    i64 c;
}