program raylib_test;

import <raylib.h> as raylib;

raylib::InitWindow(800, 600, "Hello from Source".data);
raylib::SetTargetFPS(60);
while not raylib::WindowShouldClose() {
    raylib::BeginDrawing();
    raylib::ClearBackground(raylib::Color(255 as i8, 0, 0, 255 as i8));
    raylib::EndDrawing();
}