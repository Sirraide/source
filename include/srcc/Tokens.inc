#ifndef TOKEN
#    define TOKEN(name, spelling)
#endif

#ifndef KEYWORD
#    define KEYWORD(name, spelling) TOKEN(name, spelling)
#endif

#ifndef PUNCTUATOR
#    define PUNCTUATOR(name, spelling) TOKEN(name, spelling)
#endif


// The invalid token has value 0.
TOKEN(Invalid, "<invalid>")

// Miscellaneous tokens.
TOKEN(Comment, "<comment>")
TOKEN(CXXHeaderName, "<cxx header name>")
TOKEN(Eof, "<eof>")
TOKEN(Identifier, "<identifier>")
TOKEN(Integer, "<integer>")
TOKEN(IntegerType, "<integer type>")
TOKEN(StringLiteral, "<string literal>")
TOKEN(TemplateType, "<template type>")
TOKEN(Unquote, "<unquote>") // Marker for an unquote that needs to be inserted here.

